{
    "name": "Group Bookmarker",
    "description": "Allows bookmarking tab groups with a single click",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "bookmarks",
        "tabGroups",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}